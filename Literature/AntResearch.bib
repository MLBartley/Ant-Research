journal = {Environmetrics},
keywords = {bayesian analysis,density estimation,spatial point pattern data,strauss process},
pages = {1--9},
title = {{Probabilistic Forecasts of Solar Irradiance by Stochastic Differential Equations}},
volume = {In Press?},
