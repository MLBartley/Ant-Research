number = {3},
pages = {251--272},
title = {{Hidden Markov Models for Speech Recognition}},
url = {http://acl.ldc.upenn.edu/J/J93/J93-1016.pdf{\%}5Cnhttp://www.tandfonline.com/doi/abs/10.1080/00401706.1991.10484833},
volume = {33},
