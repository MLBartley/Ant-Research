@article{Towner2016,
abstract = {Fine-scale predator movements may be driven by many factors including$\backslash$nsex, habitat and distribution of resources. There may also be individual$\backslash$npreferences for certain movement strategies within a population which$\backslash$ncan be hard to quantify.$\backslash$nWithin top predators, movements are also going to be directly related to$\backslash$nthe mode of hunting, for example sit-and-wait or actively searching for$\backslash$nprey. Although there is mounting evidence that different hunting modes$\backslash$ncan cause opposing trophic cascades, there has been little focus on the$\backslash$nmodes used by top predators, especially those in the marine environment.$\backslash$nAdult white sharks (Carcharhodon carcharias) are well known to forage on$\backslash$nmarine mammal prey, particularly pinnipeds. Sharks primarily ambush$\backslash$npinnipeds on the surface, but there has been less focus on the$\backslash$nstrategies they use to encounter prey.$\backslash$nWe applied mixed hidden Markov models to acoustic tracking data of white$\backslash$nsharks in a coastal aggregation area in order to quantify changing$\backslash$nmovement states (area-restricted searching (ARS) vs. patrolling) and the$\backslash$nfactors that influenced them. Individuals were re-tracked over multiple$\backslash$ndays throughout a month to see whether state-switching dynamics varied$\backslash$nor if individuals preferred certain movement strategies.$\backslash$nSharks were more likely to use ARS movements in the morning and during$\backslash$nperiods of chumming by ecotourism operators. Furthermore, the proportion$\backslash$nof time individuals spent in the two different states and the$\backslash$nstate-switching frequency, differed between the sexes and between$\backslash$nindividuals.$\backslash$nPredation attempts/success on pinnipeds were observed for sharks in both$\backslash$nARS and patrolling movement states and within all random effects$\backslash$ngroupings. Therefore, white sharks can use both a sit-and-wait' (ARS)$\backslash$nand active searching' (patrolling) movements to ambush pinniped prey on$\backslash$nthe surface.$\backslash$nWhite sharks demonstrate individual preferences for fine-scale movement$\backslash$npatterns, which may be related to their use of different hunting modes.$\backslash$nMarine top predators are generally assumed to use only one type of$\backslash$nhunting mode, but we show that there may be a mix within populations. As$\backslash$nsuch, individual variability should be considered when modelling$\backslash$nbehavioural effects of predators on prey species.},
author = {Towner, Alison V. and Leos-Barajas, Vianey and Langrock, Roland and Schick, Robert S. and Smale, Malcolm J. and Kaschke, Tami and Jewell, Oliver J.D. and Papastamatiou, Yannis P.},
doi = {10.1111/1365-2435.12613},
file = {:Users/mlbartley/Documents/Mendeley Desktop/Towner et al. - 2016.pdf:pdf},
isbn = {1365-2435},
issn = {13652435},
journal = {Funct. Ecol.},
keywords = {acoustic telemetry,fine-scale movement,foraging mode,hidden Markov Model,predator–prey},
number = {8},
pages = {1397--1407},
title = {{Sex-specific and individual preferences for hunting strategies in white sharks}},
volume = {30},
year = {2016}
}
@article{Chen2014,
abstract = {Contact patterns among hosts are considered as one of the most critical factors contributing to unequal pathogen transmission. Consequently, networks have been widely applied in infectious disease modeling. However most studies assume static network structure due to lack of accurate observation and appropriate analytic tools. In this study we used high temporal and spatial resolution animal position data to construct a high-resolution contact network relevant to infectious disease transmission. The animal contact network aggregated at hourly level was highly variable and dynamic within and between days, for both network structure (network degree distribution) and individual rank of degree distribution in the network (degree order). We integrated network degree distribution and degree order heterogeneities with a commonly used contact-based, directly transmitted disease model to quantify the effect of these two sources of heterogeneity on the infectious disease dynamics. Four conditions were simulated based on the combination of these two heterogeneities. Simulation results indicated that disease dynamics and individual contribution to new infections varied substantially among these four conditions under both parameter settings. Changes in the contact network had a greater effect on disease dynamics for pathogens with smaller basic reproduction number (i.e. R0 {\textless} 2).},
author = {Chen, Shi and White, Brad J and Sanderson, Michael W and Amrine, David E and Ilany, Amiyaal and Lanzas, Cristina},
doi = {10.1038/srep04472},
file = {:Users/mlbartley/Documents/Mendeley Desktop/Chen et al. - 2014.pdf:pdf},
issn = {2045-2322},
journal = {Sci. Rep.},
pages = {4472},
pmid = {24667241},
title = {{Highly dynamic animal contact network and implications on disease transmission.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3966050{\&}tool=pmcentrez{\&}rendertype=abstract},
volume = {4},
year = {2014}
}
