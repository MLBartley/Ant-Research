isbn = {0024-3590},
issn = {00243590},
journal = {Limnol. Oceanogr.},
number = {5},
pages = {1691--1703},
title = {{Regional variability among nonlinear chlorophyll-phosphorus relationships in lakes}},
url = {http://doi.wiley.com/10.4319/lo.2014.59.5.1691},
volume = {59},
year = {2014}
}
